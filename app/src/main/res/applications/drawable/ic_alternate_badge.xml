<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:centerColor="#000"
                android:gradientRadius="11dp"
                android:startColor="#000"
                android:type="radial" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <solid android:color="?attr/colorSecondaryContainer" />
        </shape>
    </item>
    <item
        android:bottom="6dp"
        android:drawable="@drawable/ic_shared_folder"
        android:left="6dp"
        android:right="6dp"
        android:top="6dp" />
</layer-list>